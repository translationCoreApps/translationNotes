{
  "name": "translationNotes",
  "version": "1.0.2",
  "description": "A tool to check a wide range of potential translation problems in the Bible, including grammatical constructions and figures of speech that can be difficult to translate, with explanations and suggestions for translation.",
  "title": "translationNotes (Part 1)",
  "include": {
    "TranslationHelps": "1.0.0",
    "ScripturePane": "1.0.2",
    "VerseCheck": "1.0.0"
  },
  "main": "index.js",
  "display": "app",
  "scripts": {
    "test": "eslint ./src ./index.js && jest"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "setupFiles": [
      "./testSetUp/shim.js",
      "./testSetUp/enzyme.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/translationCoreApps/translationNotes_Check_plugin.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/translationCoreApps/translationNotes_Check_plugin/issues"
  },
  "homepage": "https://github.com/translationCoreApps/translationNotes_Check_plugin#readme",
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^21.1.0",
    "react-test-renderer": "^15.6.2"
  },
  "dependencies": {
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "natural": "^0.4.0",
    "regexp": "^1.0.0",
    "xregexp": "^3.1.1"
  }
}
